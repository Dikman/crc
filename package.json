{
  "name": "demo",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "start": "ng serve",
    "build": "ng build --prod",
    "lint": "ng lint",
    "test": "ng test",
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer ./dist/demo/stats-es2015.json",
    "lib:build": "ng build crc",
    "lib:watch": "ng build crc --watch",
    "lib:lint": "ng lint crc",
    "lib:test": "ng test crc",
    "lib:coverage": "ng test crc --no-watch --code-coverage",
    "lib:contents": "doctoc ./projects/crc/README.md",
    "lib:docs": "cd ./projects/crc && compodoc -p tsconfig.lib.json  -n \"CRC library\" -s -o --watch --theme stripe --disablePrivate --disableProtected --disableInternal --disableLifeCycleHooks --disableSourceCode --hideGenerator -d ../../dist/docs",
    "lib:publish": "npm run lib:contents && npm run lib:build && cp ./projects/crc/CHANGELOG.md ./dist/crc/CHANGELOG.md && cd ./dist/crc && npm pack && npm publish --access public"
  },
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "ngx-highlightjs": "^4.0.2",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.22",
    "@angular-devkit/build-ng-packagr": "~0.803.22",
    "@angular/cli": "~8.3.22",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "doctoc": "^1.4.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
